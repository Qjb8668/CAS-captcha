{
    "manifest_version": 2,
    "name": "SYSU NetID captcha recognization",
    "version": "0.1",
    "description": "This is a Chrome Extention for recognizing and filling the captcha in NetID website",
    "icons":
    {
        "32": "icon.png"
    },
    "content_scripts":
    [
        {
            "matches": ["https://cas.sysu.edu.cn/*"],
            "js": ["jquery-3.4.1.min.js", "main.js"],
            "run_at": "document_start",
            "all_frame": true
        }
    ],
    "permissions": [
        "tabs"
    ],
    "homepage_url": "https://github.com/Qjb8668",
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_title": "Hi!",
        "default_popup": "popup.html"
    }
}